{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main",
    "webview_*"
  ],
  "remote": {
    "urls": [
      "https://*",
      "http://*"
    ]
  },
  "permissions": [
    "core:default",
    "core:webview:allow-create-webview",
    "core:webview:allow-create-webview-window",
    "opener:default",
    "store:default"
  ]
}
